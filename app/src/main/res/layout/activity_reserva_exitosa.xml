<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/blackApp"
    tools:context=".Reserva_Exitosa">

    <Button
        android:id="@+id/button27"
        android:layout_width="355dp"
        android:layout_height="65dp"
        android:layout_marginTop="52dp"
        android:backgroundTint="@color/blueApp"
        android:onClick="MenuPrincipal"
        android:text="@string/MenuPrincipal"
        app:cornerRadius="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView78" />

    <TextView
        android:id="@+id/textView78"
        android:layout_width="327dp"
        android:layout_height="454dp"
        android:layout_marginTop="80dp"
        android:background="@color/whiteApp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView79"
        android:layout_width="311dp"
        android:layout_height="61dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="8dp"
        android:gravity="center"
        android:text="@string/ReservaExitosa"
        android:textColor="@color/blueApp"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="@+id/textView78"
        app:layout_constraintTop_toTopOf="@+id/textView78" />

    <TextView
        android:id="@+id/textView81"
        android:layout_width="299dp"
        android:layout_height="56dp"
        android:layout_marginTop="28dp"
        android:text="@string/reservaexitosadescrip"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/textView78"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/textView78"
        app:layout_constraintTop_toBottomOf="@+id/textView79" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="173dp"
        android:layout_height="161dp"
        android:layout_marginTop="32dp"
        android:background="@mipmap/reservaexitosa"
        app:layout_constraintEnd_toEndOf="@+id/textView78"
        app:layout_constraintStart_toStartOf="@+id/textView78"
        app:layout_constraintTop_toBottomOf="@+id/textView81"
        app:srcCompat="@mipmap/reservaexitosa" />

</androidx.constraintlayout.widget.ConstraintLayout>