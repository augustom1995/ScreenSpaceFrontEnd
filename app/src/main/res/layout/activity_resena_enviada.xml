<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/blackApp"
    tools:context=".Resena_Enviada">

    <TextView
        android:id="@+id/textView106"
        android:layout_width="327dp"
        android:layout_height="454dp"
        android:layout_marginTop="80dp"
        android:background="@color/whiteApp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView104"
        android:layout_width="311dp"
        android:layout_height="61dp"
        android:layout_marginTop="28dp"
        android:gravity="center"
        android:text="@string/ResenaEnviada"
        android:textColor="@color/blueApp"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="@+id/textView106"
        app:layout_constraintStart_toStartOf="@+id/textView106"
        app:layout_constraintTop_toTopOf="@+id/textView106" />

    <TextView
        android:id="@+id/textView105"
        android:layout_width="299dp"
        android:layout_height="56dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="@string/ResenaDesc"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/textView106"
        app:layout_constraintStart_toStartOf="@+id/textView106"
        app:layout_constraintTop_toBottomOf="@+id/textView104" />

    <Button
        android:id="@+id/button37"
        android:layout_width="355dp"
        android:layout_height="65dp"
        android:layout_marginTop="60dp"
        android:backgroundTint="@color/blueApp"
        android:onClick="MenuPricipal"
        android:text="@string/MenuPrincipal"
        app:cornerRadius="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView106" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="217dp"
        android:layout_height="200dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="@+id/textView106"
        app:layout_constraintStart_toStartOf="@+id/textView106"
        app:layout_constraintTop_toBottomOf="@+id/textView105"
        app:srcCompat="@mipmap/resenaenviada" />
</androidx.constraintlayout.widget.ConstraintLayout>